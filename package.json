    "@babel/preset-react": "^7.7.4",
    "@bigtest/cli": "^0.2.2",
    "@bigtest/interactor": "^0.9.2",
    "@bigtest/mocha": "^0.5.2",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^6.1.0",
    "@folio/stripes": "^8.0.0",
    "@folio/stripes-cli": "^2.4.0",
    "@formatjs/cli": "^4.2.16",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.8.1",
    "babel-jest": "^26.3.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "eslint": "^7.32.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jest-dom": "3.2.2",
    "eslint-plugin-testing-library": "4.6.0",
    "faker": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-junit": "^12.0.0",
    "miragejs": "^0.1.32",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.7.1",
    "react-query": "^3.23.2",
    "react-redux": "^8.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-prop-types": "^1.0.4",
    "react-test-renderer": "^16.5.1",
    "redux": "^4.0.0",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "~4.0.0",
    "lodash": "^4.17.5",
    "moment-timezone": "^0.5.20",
    "prop-types": "^15.5.10",
    "query-string": "^6.1.0",
    "react-final-form": "^6.5.1",
    "react-final-form-arrays": "^3.1.1",
    "redux-form": "^8.3.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.7.1",
    "react-redux": "^8.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0"
  },
  "optionalDependencies": {
    "@folio/plugin-find-contact": "^4.0.0",
    "@folio/plugin-find-interface": "^4.0.0"
  }
}
